(<= (filled ?c ?h) (true (cell ?c ?h r)))
(<= (next (cell ?c ?h2 w)) (succ ?h1 ?h2) (does white (drop ?c)) (true (cell ?c ?h2 b)) (filled ?c ?h1))
(<= (next (cell ?c ?h2 r)) (succ ?h1 ?h2) (does red (drop ?c)) (true (cell ?c ?h2 b)) (filled ?c ?h1))
