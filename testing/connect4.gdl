(<= (next (control red)) (true (control white)))
(<= (row ?z) (mergeperm1 ?x1 ?x2 ?x3 ?x4) (true (cell ?x1 ?y ?z)) (distinct ?z b) (distinct ?z dirt) (true (cell ?x2 ?y ?z)) (true (cell ?x3 ?y ?z)) (true (cell ?x4 ?y ?z)))
(<= (column ?z) (mergeperm1 ?y1 ?y2 ?y3 ?y4) (true (cell ?x ?y1 ?z)) (distinct ?z b) (true (cell ?x ?y2 ?z)) (true (cell ?x ?y3 ?z)) (true (cell ?x ?y4 ?z)))
(<= (diag ?z) (mergeperm1 ?x1 ?x2 ?x3 ?x4) (mergeperm1 ?y1 ?y2 ?y3 ?y4) (true (cell ?x1 ?y1 ?z)) (distinct ?z b) (true (cell ?x2 ?y2 ?z)) (true (cell ?x3 ?y3 ?z)) (true (cell ?x4 ?y4 ?z)))
(<= (diag ?z) (mergeperm1 ?x1 ?x2 ?x3 ?x4) (mergeperm2 ?y1 ?y2 ?y3 ?y4) (true (cell ?x1 ?y1 ?z)) (distinct ?z b) (true (cell ?x2 ?y2 ?z)) (true (cell ?x3 ?y3 ?z)) (true (cell ?x4 ?y4 ?z)))
(<= (line ?x) (row ?x))
(<= (line ?x) (column ?x))
(<= (line ?x) (diag ?x))
(<= open (empty ?c ?h))
(<= terminal (line r))
(<= terminal (line w))
(<= terminal (not open))
(<= (legal ?w (drop ?c)) (succ ?y1 ?y2) (true (cell ?c ?y2 b)) (filled ?c ?y1) (true (control ?w)))
(<= (legal white noop) (true (control red)))
(<= (legal red noop) (true (control white)))
